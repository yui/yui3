<?xml version="1.0" encoding="UTF-8"?>
<project name="componentbuild" default="local">
    <description>YUI Core Build</description>

    <target name="local">
        <ant antfile="../yui/build.xml" target="local" useNativeBasedir="true" />
    </target>
    <target name="deploy">
        <ant antfile="../yui/build.xml" target="deploy" useNativeBasedir="true" />
    </target>
    <target name="all">
        <ant antfile="../yui/build.xml" target="all" useNativeBasedir="true" />
    </target>
    <target name="clean">
        <ant antfile="../yui/build.xml" target="clean" useNativeBasedir="true" />
    </target>

</project>
