{
    "event": {
        "after": "node-base",
        "submodules": {
            "event-base": {
                "after": "node-base",
                "requires": [
                    "event-custom-base"
                ]
            },
            "event-synthetic": {
                "requires": [
                    "node-base",
                    "event-custom-complex"
                ]
            },
            "event-delegate": {
                "requires": [
                    "node-base"
                ]
            },
            "event-focus": {
                "requires": [
                    "event-synthetic"
                ]
            },
            "event-key": {
                "requires": [
                    "event-synthetic"
                ]
            },
            "event-mouseenter": {
                "requires": [
                    "event-synthetic"
                ]
            },
            "event-mousewheel": {
                "requires": [
                    "event-synthetic"
                ]
            },
            "event-resize": {
                "requires": [
                    "event-synthetic"
                ]
            }
        },
        "plugins": {
            "event-touch": {
                "requires" : [
                     "node-base"
                ]
            },
            "event-base-ie": {
                "requires": [
                    "node-base"
                ],
                "after": [
                    "event-base"
                ],
                "condition": {
                   "trigger": "node-base",
                   "ua": "ie"
                }
            }
        }
    }
}
