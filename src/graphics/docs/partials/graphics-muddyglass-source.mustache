            },
            fill: {
                type: "linear",
                rotation: 0,
                stops: [
                    {color: "#d1c4bd", opacity:0.9, offset: 0},
                    {color: "#a78c7e", opacity: 0.9, offset: 0.4},
                    {color: "#6c3f27", opacity: 0.9, offset: 0.7}
