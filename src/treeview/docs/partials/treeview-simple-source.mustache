
<div id="demo" class="yui3-skin-sam  yui3-cssreset">
    <div id="mytree"></div>
</div>

<script type="text/javascript">
YUI().use('treeview',  function(Y) {
    
    // Create a new instance of treeview, placing it in 
    // #mytree container   
    
    var tree = new Y.TreeView({
       label : "This my tree",
       children: [
           { label: "Leaf One" },
           {label: "Leaf Two"}
       ]
    });
    
    tree.render("#mytree");
});
</script>
