.example .yui3-panel {
  outline:none;
}

.example .yui3-panel #panelContent {
  -webkit-box-shadow: 0px 0px 2px black;
  -moz-box-shadow: 0px 0px 2px black;
  box-shadow: 0px 0px 2px black;
}

.example .yui3-widget-hd, .example .yui3-widget-ft {
  font-weight:bold;
  padding:5px;
  background:white;

}

.example #panelContent .yui3-widget-bd {
  padding:15px;
  background:white;
  text-align:center;
}

.yui3-widget-mask {
  background-color: #223460;
  opacity: 0.9;
}
