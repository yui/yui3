{
    "yui": {
        "use": [ "yui-base", "get", "features", "intl-base", "yui-log", "yui-later", "loader-base","loader-rollup","loader-yui3" ],
        "submodules": {
            "get": {
                "requires": ["yui-base"]
            },
            "yui-base": {},
            "yui-rls": {
                "use": [ "yui-base", "get", "features", "intl-base", "rls", "yui-log", "yui-later" ]
            },
            "rls": {
                "requires": ["get", "features"]
            },
            "features": {
                "requires": ["yui-base"]
            },
            "yui-later": {
                "requires": ["yui-base"]
            },
            "yui-log": {
                "requires": ["yui-base"]
            },
            "intl-base": {
                "requires": ["yui-base"]
            }
        }
    }
}
