{
    "datatable": {
        "use": [
            "datatable-core",
            "datatable-table",
            "datatable-head",
            "datatable-body",
            "datatable-base",
            "datatable-column-widths",
            "datatable-message",
            "datatable-mutable",
            "datatable-sort",
            "datatable-datasource"
        ],
        "submodules": {
            "datatable-core": {
                "requires": [
                    "escape",
                    "model-list",
                    "node-event-delegate"
                ]
            },
            "datatable-table": {
                "requires": [
                    "datatable-core",
                    "datatable-head",
                    "datatable-body",
                    "view",
                    "classnamemanager"
                ]
            },
            "datatable-head": {
                "requires": [
                    "datatable-core",
                    "view",
                    "classnamemanager"
                ]
            },
            "datatable-foot": {
                "requires": [
                    "datatable-core",
                    "view"
                ]
            },
            "datatable-body": {
                "requires": [
                    "datatable-core",
                    "view",
                    "classnamemanager"
                ]
            },
            "datatable-base": {
                "requires": [
                    "datatable-core",
                    "datatable-table",
                    "datatable-head",
                    "datatable-body",
                    "base-build",
                    "widget"
                ],
                "skinnable": true
            },
            "datatable-column-widths": {
                "requires": [
                "datatable-base"
                ]
            },
            "datatable-message": {
                "requires": [
                    "datatable-base"
                ],
                "lang": ["en", "fr", "es", "hu", "it"],
                "skinnable": true
            },
            "datatable-mutable": {
                "requires": [
                    "datatable-base"
                ]
            },
            "datatable-datasource": {
                "requires": [
                    "datatable-base",
                    "plugin",
                    "datasource-local"
                    ]
            },
            "datatable-sort": {
                "requires": [
                    "datatable-base"
                ],
                "lang": ["en", "fr", "es", "hu"],
                "skinnable": true
            },
            "datatable-scroll": {
                "requires": [
                    "datatable-base",
                    "datatable-column-widths",
                    "dom-screen"
                ],
                "skinnable": true
            },
            "datatable-formatters": {
                "requires": [
                    "datatable-body",
                    "datatype-number-format",
                    "datatype-date-format",
                    "escape"
                ]
            },
            "datatable-paginator": {
                "requires": [
                    "model",
                    "view",
                    "paginator-core",
                    "datatable-foot",
                    "datatable-paginator-templates"
                ],
                "lang": ["en", "fr"],
                "skinnable": true
            },
            "datatable-paginator-templates": {
                "requires": [
                    "template"
                ]
            },
            "datatable-keynav": {
                "requires": [
                    "datatable-base"
                ]
            },
            "datatable-highlight": {
                "requires": [
                    "datatable-base",
                    "event-hover"
                ],
                "skinnable": true
            }
        }
    }
}
