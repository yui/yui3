{
    "yql": {
        "requires": [
            "jsonp",
            "jsonp-url"
        ],
        "plugins": {
            "yql-winjs": {
                "condition": {
                    "trigger": "yql",
                    "ua": "winjs",
                    "when": "after"
                }
            },
            "yql-nodejs": {
                "condition": {
                    "trigger": "yql",
                    "ua": "nodejs",
                    "when": "after"
                }
            }
        }
    }
}
